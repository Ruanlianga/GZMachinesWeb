<%@ page contentType="text/html;charset=UTF-8" %>
<div id="auDiv" class="hide">
		<form id="auForm" method="POST" onsubmit="return false;" >
			<table cellspacing="0" cellpadding="0" border="0" class="customTable">
				<tbody>
					<tr style="display:none">
						<td colspan="2" class="ui-state-error"><input type="hidden" name="id" ></td>
					</tr>
					
					<tr class="FormData">
						<td class="CaptionTD"><font color="red">*</font>厂家名称：</td>
						<td class="DataTD">&nbsp;
						<input type="text" jyValidate="required"   name="name" class="FormElement ui-widget-content ui-corner-all"></td>
					</tr>
					
					<tr class="FormData">
						<td class="CaptionTD"><font color="red">*</font>厂家地址：</td>
						<td class="DataTD">&nbsp;
						<input type="text" jyValidate="required"   name="address" class="FormElement ui-widget-content ui-corner-all"></td>
					</tr>
					
					<tr class="FormData">
						<td class="CaptionTD"><font color="red">*</font>法人代表：</td>
						<td class="DataTD">&nbsp;
						<input type="text" jyValidate="required"   name="companyMan" class="FormElement ui-widget-content ui-corner-all"></td>
					</tr>
					
					<tr class="FormData">
						<td class="CaptionTD"><font color="red">*</font>主要联系人：</td>
						<td class="DataTD">&nbsp;
						<input type="text" jyValidate="required"   name="mainPerson" class="FormElement ui-widget-content ui-corner-all"></td>
					</tr>
					
					<tr class="FormData">
						<td class="CaptionTD"><font color="red">*</font>联系电话：</td>
						<td class="DataTD">&nbsp;
						<input type="text" jyValidate="required" id="phone"  name="phone" class="FormElement ui-widget-content ui-corner-all"></td>
					</tr>
					
					<tr class="FormData">
						<td class="CaptionTD"><font color="red">*</font>主要经营范围：</td>
						<td class="DataTD">&nbsp;
							<textarea rows="3" cols="21" id="scopeBusiness" jyValidate="required" name="scopeBusiness" class="FormElement ui-widget-content ui-corner-all"></textarea>
						</td>
					</tr>
					
					<tr class="FormData">
						<td class="CaptionTD">备注：</td>
						<td class="DataTD">&nbsp;
							<textarea rows="3" cols="21" id="remark" jyValidate="required" name="remark" class="FormElement ui-widget-content ui-corner-all"></textarea>
						</td>
					</tr>
				</tbody>
			</table>
		</form>
</div>